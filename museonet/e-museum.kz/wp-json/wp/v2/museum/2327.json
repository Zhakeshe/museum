u0431\u044b\u043b \u0432\u043f\u0435\u0440\u0432\u044b\u0435 \u043e\u0440\u0433\u0430\u043d\u0438\u0437\u043e\u0432\u0430\u043d \u0432 2006 \u0433\u043e\u0434\u0443 \u043a\u0430\u043a \u043a\u0440\u0430\u0435\u0432\u0435\u0434\u0447\u0435\u0441\u043a\u0438\u0439 \u0441\u0435\u043a\u0442\u043e\u0440 \u0432 \u0441\u043e\u0441\u0442\u0430\u0432\u0435 \u0446\u0435\u043d\u0442\u0440\u0430\u043b\u0438\u0437\u043e\u0432\u0430\u043d\u043d\u043e\u0439 \u0431\u0438\u0431\u043b\u0438\u043e\u0442\u0435\u0447\u043d\u043e\u0439 \u0441\u0438\u0441\u0442\u0435\u043c\u044b \u043e\u0442\u0434\u0435\u043b\u0430 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u044b \u0438 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u044f \u044f\u0437\u044b\u043a\u043e\u0432 \u041d\u0443\u0440\u0430\u0439\u0441\u043a\u043e\u0433\u043e \u0440\u0430\u0439\u043e\u043d\u0430. \u041e\u0441\u043d\u043e\u0432\u0430\u0442\u0435\u043b\u0435\u043c \u0438 \u043f\u0435\u0440\u0432\u044b\u043c \u0437\u0430\u0432\u0435\u0434\u0443\u044e\u0449\u0438\u043c \u0441\u0435\u043a\u0442\u043e\u0440\u0430 \u0441\u0442\u0430\u043b\u0430 \u0415\u0441\u043c\u0430\u0433\u0430\u043c\u0431\u0435\u0442\u043e\u0432\u0430 \u04d8\u0434\u0438\u0448\u0430 \u041a\u04e9\u043d\u0435\u043a\u0431\u0430\u0435\u0432\u043d\u0430. \u041f\u043e\u0434 \u0435\u0451 \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0441\u0442\u0432\u043e\u043c \u0431\u044b\u043b\u0438 \u0441\u043e\u0431\u0440\u0430\u043d\u044b \u043c\u043d\u043e\u0433\u043e\u0447\u0438\u0441\u043b\u0435\u043d\u043d\u044b\u0435 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u044c\u043d\u044b\u0435 \u0438 \u0434\u0443\u0445\u043e\u0432\u043d\u044b\u0435 \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u0438: \u0438\u0441\u0442\u043e\u0440\u0438\u0447\u0435\u0441\u043a\u0438\u0435 \u0434\u0430\u043d\u043d\u044b\u0435, \u0444\u043e\u0442\u043e\u0434\u043e\u043a\u0443\u043c\u0435\u043d\u0442\u044b, \u044d\u043a\u0441\u043f\u043e\u043d\u0430\u0442\u044b \u0438 \u0441\u0432\u0435\u0434\u0435\u043d\u0438\u044f, \u0432 \u0442\u043e\u043c \u0447\u0438\u0441\u043b\u0435 \u043e \u0432\u0435\u0442\u0435\u0440\u0430\u043d\u0430\u0445 \u0412\u0435\u043b\u0438\u043a\u043e\u0439 \u041e\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u0432\u043e\u0439\u043d\u044b \u0438 \u0443\u0447\u0430\u0441\u0442\u043d\u0438\u043a\u0430\u0445 \u0410\u0444\u0433\u0430\u043d\u0441\u043a\u043e\u0439 \u0432\u043e\u0439\u043d\u044b. \u0412 \u0441\u0435\u043a\u0442\u043e\u0440\u0435 \u0442\u0440\u0443\u0434\u0438\u043b\u0438\u0441\u044c \u043d\u0430\u0443\u0447\u043d\u044b\u0439 \u0441\u043e\u0442\u0440\u0443\u0434\u043d\u0438\u043a \u0438 \u0441\u043f\u0435\u0446\u0438\u0430\u043b\u0438\u0441\u0442\u044b \u043f\u043e \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044e \u0444\u043e\u043d\u0434\u043e\u0432.<\/p>\n<p>\u0412 \u043d\u0430\u0441\u0442\u043e\u044f\u0449\u0435\u0435 \u0432\u0440\u0435\u043c\u044f \u043e\u0431\u0449\u0430\u044f \u043f\u043b\u043e\u0449\u0430\u0434\u044c \u043c\u0443\u0437\u0435\u044f \u0441\u043e\u0441\u0442\u0430\u0432\u043b\u044f\u0435\u0442 875 \u043a\u0432\u0430\u0434\u0440\u0430\u0442\u043d\u044b\u0445 \u043c\u0435\u0442\u0440\u043e\u0432. \u0424\u043e\u043d\u0434 \u043c\u0443\u0437\u0435\u044f \u043d\u0430\u0441\u0447\u0438\u0442\u044b\u0432\u0430\u0435\u0442 3452 \u0435\u0434\u0438\u043d\u0438\u0446\u044b \u0445\u0440\u0430\u043d\u0435\u043d\u0438\u044f, \u0438\u0437 \u043d\u0438\u0445 \u043e\u0441\u043d\u043e\u0432\u043d\u043e\u0439 \u0444\u043e\u043d\u0434 - 1965 \u0435\u0434\u0438\u043d\u0438\u0446, \u043d\u0430\u0443\u0447\u043d\u043e-\u0432\u0441\u043f\u043e\u043c\u043e\u0433\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u0444\u043e\u043d\u0434 - 1486 \u0435\u0434\u0438\u043d\u0438\u0446.<\/p>\n<p>\u042d\u043a\u0441\u043f\u043e\u0437\u0438\u0446\u0438\u043e\u043d\u043d\u044b\u0435 \u0437\u0430\u043b\u044b \u043c\u0443\u0437\u0435\u044f:<\/p>\n<p>1. \u0417\u0430\u043b \u00ab\u0410\u0440\u0445\u0435\u043e\u043b\u043e\u0433\u0438\u044f \u0438 \u044d\u0442\u043d\u043e\u0433\u0440\u0430\u0444\u0438\u044f\u00bb - \u0412 \u044d\u0442\u043e\u043c \u0437\u0430\u043b\u0435 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u044b \u044d\u043a\u0441\u043f\u043e\u043d\u0430\u0442\u044b, \u043f\u043e\u0432\u0435\u0441\u0442\u0432\u0443\u044e\u0449\u0438\u0435 \u043e \u0434\u0440\u0435\u0432\u043d\u0438\u0445 \u044d\u043f\u043e\u0445\u0430\u0445:<\/p>\n<p>\u2022\t\u042d\u043b\u0435\u043c\u0435\u043d\u0442\u044b \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u044c\u043d\u043e\u0439 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u044b \u043a\u043e\u0447\u0435\u0432\u044b\u0445 \u043d\u0430\u0440\u043e\u0434\u043e\u0432: \u0442\u0440\u0430\u0434\u0438\u0446\u0438\u043e\u043d\u043d\u043e\u0435 \u043e\u0440\u0443\u0436\u0438\u0435 \u0438 \u0432\u043e\u043e\u0440\u0443\u0436\u0435\u043d\u0438\u0435 (\u00ab\u0435\u0440 \u049b\u0430\u0440\u0443\u044b \u2013 \u0431\u0435\u0441 \u049b\u0430\u0440\u0443\u00bb), \u043a\u043e\u043d\u0441\u043a\u0430\u044f \u0443\u043f\u0440\u044f\u0436\u044c, \u044e\u0432\u0435\u043b\u0438\u0440\u043d\u044b\u0435 \u0438\u0437\u0434\u0435\u043b\u0438\u044f \u0438 \u043f\u0440\u0435\u0434\u043c\u0435\u0442\u044b \u0434\u0435\u043a\u043e\u0440\u0430\u0442\u0438\u0432\u043d\u043e-\u043f\u0440\u0438\u043a\u043b\u0430\u0434\u043d\u043e\u0433\u043e \u0438\u0441\u043a\u0443\u0441\u0441\u0442\u0432\u0430;<\/p>\n<p>\u2022\t\u0418\u043d\u0442\u0435\u0440\u044c\u0435\u0440 \u0442\u0440\u0430\u0434\u0438\u0446\u0438\u043e\u043d\u043d\u043e\u0433\u043e \u0436\u0438\u043b\u0438\u0449\u0430 \u0442\u044e\u0440\u043a\u0441\u043a\u0438\u0445 \u043a\u043e\u0447\u0435\u0432\u044b\u0445 \u043d\u0430\u0440\u043e\u0434\u043e\u0432 - \u044e\u0440\u0442\u044b;<\/p>\n<p>\u2022\t\u041e\u0431\u0440\u0430\u0437\u0446\u044b \u043c\u0435\u0442\u0430\u043b\u043b\u0443\u0440\u0433\u0438\u0438 \u0440\u0430\u043d\u043d\u0435\u0433\u043e \u0436\u0435\u043b\u0435\u0437\u043d\u043e\u0433\u043e \u0432\u0435\u043a\u0430;<\/p>\n<p>\u2022\t\u0418\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f \u043e\u0431 \u0430\u0440\u0445\u0435\u043e\u043b\u043e\u0433\u0438\u0447\u0435\u0441\u043a\u0438\u0445 \u0438 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u043d\u044b\u0445 \u043f\u0430\u043c\u044f\u0442\u043d\u0438\u043a\u0430\u0445, \u0440\u0430\u0441\u043f\u043e\u043b\u043e\u0436\u0435\u043d\u043d\u044b\u0445 \u043d\u0430 \u0442\u0435\u0440\u0440\u0438\u0442\u043e\u0440\u0438\u0438 \u0440\u0430\u0439\u043e\u043d\u0430;<\/p>\n<p>\u2022\t\u041c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u044b \u043e \u043f\u0440\u0438\u0440\u043e\u0434\u043d\u044b\u0445 \u0440\u0435\u0441\u0443\u0440\u0441\u0430\u0445, \u0444\u043b\u043e\u0440\u0435 \u0438 \u0444\u0430\u0443\u043d\u0435 \u041d\u0443\u0440\u0430\u0439\u0441\u043a\u043e\u0433\u043e \u0440\u0430\u0439\u043e\u043d\u0430.<\/p>\n<p>2. \u0417\u0430\u043b \u00ab\u0421\u043b\u0430\u0432\u0430 \u0433\u0435\u0440\u043e\u0435\u0432\u00bb - \u0417\u0430\u043b \u043f\u043e\u0441\u0432\u044f\u0449\u0451\u043d \u0433\u0435\u0440\u043e\u0438\u0447\u0435\u0441\u043a\u043e\u043c\u0443 \u043f\u0443\u0442\u0438 \u0441\u044b\u043d\u043e\u0432 \u0438 \u0434\u043e\u0447\u0435\u0440\u0435\u0439 \u041d\u0443\u0440\u0430\u0439\u0441\u043a\u043e\u0433\u043e \u043a\u0440\u0430\u044f:<\/p>\n<p>\u2022\t\u0423\u0447\u0430\u0441\u0442\u0438\u0435 \u0436\u0438\u0442\u0435\u043b\u0435\u0439 \u0440\u0430\u0439\u043e\u043d\u0430 \u0432 \u0412\u0435\u043b\u0438\u043a\u043e\u0439 \u041e\u0442\u0435\u0447\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e\u0439 \u0432\u043e\u0439\u043d\u0435, \u0441\u0432\u0435\u0434\u0435\u043d\u0438\u044f \u043e \u0432\u0435\u0442\u0435\u0440\u0430\u043d\u0430\u0445, \u0431\u044b\u0432\u0448\u0438\u0445 \u0443\u0437\u043d\u0438\u043a\u0430\u0445 \u0444\u0430\u0448\u0438\u0441\u0442\u0441\u043a\u0438\u0445 \u043b\u0430\u0433\u0435\u0440\u0435\u0439;<\/p>\n<p>\u2022\t\u0418\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f \u043e\u0431 \u0443\u0447\u0430\u0441\u0442\u043d\u0438\u043a\u0430\u0445 \u0410\u0444\u0433\u0430\u043d\u0441\u043a\u043e\u0439 \u0432\u043e\u0439\u043d\u044b \u0438 \u0441\u043e\u0431\u044b\u0442\u0438\u0439 \u0434\u0435\u043a\u0430\u0431\u0440\u044f 1986 \u0433\u043e\u0434\u0430 (\u0416\u0435\u043b\u0442\u043e\u043a\u0441\u0430\u043d);<\/p>\n<p>\u2022\t\u0411\u0438\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u0438 \u0434\u043e\u0441\u0442\u0438\u0436\u0435\u043d\u0438\u044f \u0432\u0438\u0434\u043d\u044b\u0445 \u043b\u0438\u0447\u043d\u043e\u0441\u0442\u0435\u0439, \u0432\u043d\u0451\u0441\u0448\u0438\u0445 \u0432\u043a\u043b\u0430\u0434 \u0432 \u0441\u043e\u0446\u0438\u0430\u043b\u044c\u043d\u043e-\u044d\u043a\u043e\u043d\u043e\u043c\u0438\u0447\u0435\u0441\u043a\u043e\u0435 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u0435 \u043d\u0430 \u0440\u0435\u0441\u043f\u0443\u0431\u043b\u0438\u043a\u0430\u043d\u0441\u043a\u043e\u043c, \u043e\u0431\u043b\u0430\u0441\u0442\u043d\u043e\u043c \u0438 \u0440\u0430\u0439\u043e\u043d\u043d\u043e\u043c \u0443\u0440\u043e\u0432\u043d\u044f\u0445, \u0430 \u0442\u0430\u043a\u0436\u0435 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u044b \u043e \u00ab\u0413\u043e\u0440\u0434\u043e\u0441\u0442\u0438 \u041d\u0443\u0440\u044b\u00bb.<\/p>\n<p>3. \u0417\u0430\u043b \u00ab\u0417\u0435\u0440\u043d\u043e\u00bb - \u041d\u0443\u0440\u0430\u0439\u0441\u043a\u0438\u0439 \u0440\u0430\u0439\u043e\u043d \u0438\u0437\u0432\u0435\u0441\u0442\u0435\u043d \u043a\u0430\u043a \u043e\u0434\u0438\u043d \u0438\u0437 \u043a\u0440\u0443\u043f\u043d\u0435\u0439\u0448\u0438\u0445 \u0437\u0435\u0440\u043d\u043e\u0441\u0435\u044e\u0449\u0438\u0445 \u0440\u0435\u0433\u0438\u043e\u043d\u043e\u0432 \u043e\u0431\u043b\u0430\u0441\u0442\u0438. \u0412 \u044d\u0442\u043e\u043c \u0437\u0430\u043b\u0435 \u043f\u0440\u0435\u0434\u0441\u0442\u0430\u0432\u043b\u0435\u043d\u044b:<\/p>\n<p>\u2022\t\u0420\u0435\u0444\u043e\u0440\u043c\u044b \u0438 \u0434\u043e\u0441\u0442\u0438\u0436\u0435\u043d\u0438\u044f \u0432 \u0441\u0444\u0435\u0440\u0435 \u0441\u0435\u043b\u044c\u0441\u043a\u043e\u0433\u043e \u0445\u043e\u0437\u044f\u0439\u0441\u0442\u0432\u0430 \u0440\u0430\u0439\u043e\u043d\u0430;<\/p>\n<p>\u2022\t\u0420\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u0438 \u0441\u043e\u0432\u0445\u043e\u0437\u043e\u0432 \u043f\u0435\u0440\u0438\u043e\u0434\u0430 \u043e\u0441\u0432\u043e\u0435\u043d\u0438\u044f \u0446\u0435\u043b\u0438\u043d\u043d\u044b\u0445 \u0438 \u0437\u0430\u043b\u0435\u0436\u043d\u044b\u0445 \u0437\u0435\u043c\u0435\u043b\u044c;<\/p>\n<p>\u2022\t\u0418\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044f \u043e \u0440\u0430\u0437\u0440\u0435\u0437\u0435 \u0422\u041e\u041e \u00ab\u0428\u0443\u0431\u0430\u0440\u043a\u043e\u043b\u044c \u043a\u04e9\u043c\u0456\u0440\u00bb, \u0440\u0443\u043a\u043e\u0432\u043e\u0434\u0438\u0442\u0435\u043b\u044f\u0445 \u0416\u041a\u0425;<\/p>\n<p>\u2022\t\u0411\u0438\u043e\u0433\u0440\u0430\u0444\u0438\u0438 \u0434\u0435\u043f\u0443\u0442\u0430\u0442\u043e\u0432 \u043c\u0430\u0441\u043b\u0438\u0445\u0430\u0442\u043e\u0432, \u043a\u0430\u0432\u0430\u043b\u0435\u0440\u043e\u0432 \u043e\u0440\u0434\u0435\u043d\u043e\u0432 \u0438 \u0438\u0445 \u0442\u0440\u0443\u0434\u043e\u0432\u043e\u0439 \u043f\u0443\u0442\u044c.<\/p>\n<p>\u041e\u0441\u043d\u043e\u0432\u043d\u044b\u0435 \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u0438\u044f \u0438 \u043e\u0431\u0449\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u0430\u044f \u0434\u0435\u044f\u0442\u0435\u043b\u044c\u043d\u043e\u0441\u0442\u044c \u043c\u0443\u0437\u0435\u044f: \u041c\u0443\u0437\u0435\u0439 \u0444\u0443\u043d\u043a\u0446\u0438\u043e\u043d\u0438\u0440\u0443\u0435\u0442 \u043a\u0430\u043a \u0441\u043e\u0432\u0440\u0435\u043c\u0435\u043d\u043d\u044b\u0439 \u043a\u0443\u043b\u044c\u0442\u0443\u0440\u043d\u043e-\u043e\u0431\u0440\u0430\u0437\u043e\u0432\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0439 \u0446\u0435\u043d\u0442\u0440; \u041f\u0440\u043e\u0432\u043e\u0434\u0438\u0442 \u0438\u0441\u0442\u043e\u0440\u0438\u043a\u043e-\u043f\u043e\u0437\u043d\u0430\u0432\u0430\u0442\u0435\u043b\u044c\u043d\u044b\u0435 \u0437\u0430\u043d\u044f\u0442\u0438\u044f, \u043c\u0430\u0441\u0442\u0435\u0440-\u043a\u043b\u0430\u0441\u0441\u044b, \u00ab\u0423\u0440\u043e\u043a\u0438 \u0432 \u043c\u0443\u0437\u0435\u0435\u00bb, \u00ab\u0414\u043d\u0438 \u043e\u0442\u043a\u0440\u044b\u0442\u044b\u0445 \u0434\u0432\u0435\u0440\u0435\u0439\u00bb \u0434\u043b\u044f \u0448\u043a\u043e\u043b\u044c\u043d\u0438\u043a\u043e\u0432 \u0438 \u043c\u043e\u043b\u043e\u0434\u0435\u0436\u0438; \u0412\u0435\u0434\u0435\u0442 \u043d\u0430\u0443\u0447\u043d\u043e-\u043f\u043e\u0437\u043d\u0430\u0432\u0430\u0442\u0435\u043b\u044c\u043d\u0443\u044e \u0440\u0430\u0431\u043e\u0442\u0443 \u0447\u0435\u0440\u0435\u0437 \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u0435 \u043c\u0435\u0441\u0442\u043d\u043e\u0439 \u0438\u0441\u0442\u043e\u0440\u0438\u0438, \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u0438\u0437\u0430\u0446\u0438\u044e \u043d\u0430\u0441\u043b\u0435\u0434\u0438\u044f \u0432\u044b\u0434\u0430\u044e\u0449\u0438\u0445\u0441\u044f \u043b\u0438\u0447\u043d\u043e\u0441\u0442\u0435\u0439, \u0441\u0431\u043e\u0440 \u0430\u0440\u0445\u0438\u0432\u043d\u044b\u0445 \u0438 \u044d\u043a\u0441\u043f\u0435\u0434\u0438\u0446\u0438\u043e\u043d\u043d\u044b\u0445 \u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\u043e\u0432; \u041e\u0440\u0433\u0430\u043d\u0438\u0437\u0443\u0435\u0442 \u043f\u0440\u0430\u0437\u0434\u043d\u0438\u0447\u043d\u044b\u0435 \u0438 \u0442\u0440\u0430\u0434\u0438\u0446\u0438\u043e\u043d\u043d\u044b\u0435 \u043c\u0435\u0440\u043e\u043f\u0440\u0438\u044f\u0442\u0438\u044f, \u043d\u0430\u043f\u0440\u0430\u0432\u043b\u0435\u043d\u043d\u044b\u0435 \u043d\u0430 \u043f\u043e\u043f\u0443\u043b\u044f\u0440\u0438\u0437\u0430\u0446\u0438\u044e \u043d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u044b\u0445 \u0446\u0435\u043d\u043d\u043e\u0441\u0442\u0435\u0439.<\/p>\n<p>\u041d\u0443\u0440\u0430\u0439\u0441\u043a\u0438\u0439 \u0440\u0430\u0439\u043e\u043d\u043d\u044b\u0439 \u0438\u0441\u0442\u043e\u0440\u0438\u043a\u043e-\u043a\u0440\u0430\u0435\u0432\u0435\u0434\u0447\u0435\u0441\u043a\u0438\u0439 \u043c\u0443\u0437\u0435\u0439 - \u044d\u0442\u043e \u0443\u0447\u0440\u0435\u0436\u0434\u0435\u043d\u0438\u0435, \u0438\u0433\u0440\u0430\u044e\u0449\u0435\u0435 \u0432\u0430\u0436\u043d\u0443\u044e \u0440\u043e\u043b\u044c \u0432 \u0441\u0438\u0441\u0442\u0435\u043c\u0430\u0442\u0438\u0447\u0435\u0441\u043a\u043e\u043c \u0438\u0437\u0443\u0447\u0435\u043d\u0438\u0438 \u0438\u0441\u0442\u043e\u0440\u0438\u0438 \u0440\u0435\u0433\u0438\u043e\u043d\u0430 \u0438 \u0432 \u0441\u043e\u0445\u0440\u0430\u043d\u0435\u043d\u0438\u0438 \u0438 \u0440\u0430\u0437\u0432\u0438\u0442\u0438\u0438 \u043d\u0430\u0446\u0438\u043e\u043d\u0430\u043b\u044c\u043d\u043e\u0439 \u043f\u0430\u043c\u044f\u0442\u0438.<\/p>\n<p><\/p>\n","logo":"https:\/\/crm.e-museum.kz\/emuseum\/museum\/MuseumGal\/d3356d42-cd23-4de2-8504-d1a6a27d87f6.png","museum_type":"","multimedia":["https:\/\/crm.e-museum.kz\/emuseum\/museum\/MuseumGal\/2c9ec95d-e573-4b62-a3db-6384b0cccd68.jpg","https:\/\/crm.e-museum.kz\/emuseum\/museum\/MuseumGal\/9b340548-b9fb-49f2-9407-0609367bb69e.jpeg","https:\/\/crm.e-museum.kz\/emuseum\/museum\/MuseumGal\/db7dcc0c-8829-477b-938a-60018d7517f2.jpg","https:\/\/crm.e-museum.kz\/emuseum\/museum\/MuseumGal\/1d19424a-f0a8-4bb3-b02f-a52b2ec9e870.jpg","https:\/\/crm.e-museum.kz\/emuseum\/museum\/MuseumGal\/1df1d8b3-b2de-4082-8cb8-b5b77ef79b07.jpg","https:\/\/crm.e-museum.kz\/emuseum\/museum\/MuseumGal\/d472c96c-0261-4edb-85bd-c7d7a2fdca6b.jpg","https:\/\/crm.e-museum.kz\/emuseum\/museum\/MuseumGal\/aa7375af-39c8-4f17-b1fa-432eda484b25.jpg"],"work_hours":"09.00-18.00","ticket_price":"\u0434\u043b\u044f \u0438\u043d\u043e\u0441\u0442\u0440\u0430\u043d\u043d\u044b\u0445 \u043f\u043e\u0441\u0435\u0442\u0438\u0442\u0435\u043b\u0435\u